#include <iostream>
using namespace std;

int main () {
	int angkaAwal = 0;
	int angkaAkhir = 10;
		for ( int i = angkaAwal ; i <= angkaAkhir ; i++ ) {
			int counterPrima = 0;
			for ( int f = 1 ; f <= i ; f++ ) {
				if ( i % f == 0 ) {
					counterPrima++;
				}
			}
			if ( counterPrima == 2 ) {
				cout << i << " adalah bilangan PRIMA " << endl;
			}
		}
		return 0;
}
